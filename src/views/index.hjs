<!DOCTYPE html>
<html class="no-js">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Venue Server</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" href="css/vendor/bootstrap.css">
    <link rel="stylesheet" href="css/vendor/jquery.pnotify.default.css">
    <link rel="stylesheet" href="css/vendor/jquery.pnotify.default.icons.css">
    <link rel="stylesheet" href="css/main.css">

    <script src="js/vendor/modernizr-2.6.2-respond-1.1.0.min.js"></script>
  </head>
  <body>

    <nav class="navbar navbar-default" role="navigation">
      <div class="navbar-text">Venue Server</div>
      <div class="navbar-text navbar-right"><span class="label js-connectionState"></span></div>
    </nav>

    <div class="container">
      <div class="row">
        <div class="col-md-4">
          <div class="panel panel-default js-serverStats"></div>
          <div class="panel panel-default js-settings"></div>
        </div>
        <div class="col-md-8">
          <div class="panel panel-default js-sensorStats"></div>
        </div>
      </div> <!-- /row -->
      <div class="row">
        <div class="col-md-4 js-currentDataSet"></div>
        <div class="col-md-8 js-logList"></div>
      </div> <!-- /row -->
    </div> <!-- /container -->

    <!-- templates -->
{{#templateData}}
    <script type="text/template" id="{{id}}">
{{{contents}}}
    </script>
{{/templateData}}

    <!-- app data -->
    <script>
      var app = app || {};
      app.initData = {{{initData}}};
    </script>

    <!-- vendor -->
    <script src="/js/vendor/jquery-1.10.1.js"></script>
    <script src="/js/vendor/lodash.min.js"></script>
    <script src="/js/vendor/hogan.min.js"></script>
    <script src="/js/vendor/bootstrap.js"></script>
    <script src="/js/vendor/backbone.js"></script>
    <script src="/js/vendor/backbone.viewmaster.js"></script>
    <script src="/js/vendor/sockjs-0.3.4.js"></script>
    <script src="/js/vendor/bootstrap.js"></script>
    <script src="/js/vendor/bootstrap-lightbox.js"></script>
    <script src="/js/vendor/jquery.pnotify.js"></script>

    <!-- models -->
    <script src="/js/models/settings.js"></script>

    <!-- views -->
    <script src="/js/views/settings.js"></script>

    <!-- application -->
    <script src="/js/ctrl.js"></script>
    <script src="/js/utils.js"></script>
    <script src="/js/websocket.js"></script>
    <script src="/js/main.js"></script>
  </body>
</html>
