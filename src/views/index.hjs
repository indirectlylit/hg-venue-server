<!DOCTYPE html>
<html class="no-js">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Venue Server</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link rel="stylesheet" href="css/bootstrap.css">
    <link rel="stylesheet" href="css/main.css">

    <script src="js/vendor/modernizr-2.6.2-respond-1.1.0.min.js"></script>
  </head>
  <body>

    <nav class="navbar navbar-default" role="navigation">
      <div class="navbar-text">Venue Server</div>
      <div class="navbar-text navbar-right"><span class="label js-connection-state"></span></div>
    </nav>

    <div class="container">
      <div class="row">
        <div class="col-md-8">
          <div class="panel panel-default">
            <div class="panel-heading">
              <h3 class="panel-title">Network</h3>
            </div>
            <div class="panel-body">
              <table class="table table-condensed">
                <thead>
                  <td>Address</td>
                  <td>Actual Rate (messages/s)</td>
                  <td>Attempted Interval (ms)</td>
                  <td>kB/s</td>
                  <td>Dropped Messages</td>
                </thead>
                <tbody class="js-dataTable"></tbody>
              </table>
            </div>
          </div> <!-- /panel -->
        </div> <!-- /col -->
        <div class="col-md-4">
          <div class="panel panel-default">
            <div class="panel-heading">
              <h3 class="panel-title">Server</h3>
            </div>
            <div class="panel-body">
              <dl class="dl-horizontal">
                <dt>Architecture</dt>
                <dd class="js-server-arch"></dd>
                <dt>Server Uptime</dt>
                <dd class="js-server-uptime"></dd>
                <dt>App Uptime</dt>
                <dd class="js-server-app-uptime"></dd>
                <dt><span data-toggle="tooltip" class="helptext" title="server time - browser time">Time Skew</span></dt>
                <dd class="js-server-skew"></dd>
                <dt>RAM</dt>
                <dd class="js-server-mem"></dd>
                <dt><span data-toggle="tooltip" class="helptext" title="1, 5, 15 minute load averages">CPU Load</span></dt>
                <dd class="js-server-load"></dd>
              </dl>
            </div>
          </div> <!-- /panel -->
        </div> <!-- /col -->
      </div> <!-- /row -->
    </div> <!-- /container -->
  
    <!-- vendor -->
    <script src="/js/vendor/jquery-1.10.1.js"></script>
    <script src="/js/vendor/lodash.min.js"></script>
    <script src="/js/vendor/bootstrap.js"></script>
    <script src="/js/vendor/backbone.js"></script>
    <script src="/js/vendor/backbone.viewmaster.js"></script>
    <script src="/js/vendor/sockjs-0.3.4.js"></script>
    <script src="/js/vendor/bootstrap.js"></script>
    <script src="/js/vendor/bootstrap-lightbox.js"></script>

    <!-- models -->
    <script src="/js/models/stats.js"></script>
    <script src="/js/models/settings.js"></script>

    <!-- views -->
    <script src="/js/views/stats.js"></script>
    <script src="/js/views/settings.js"></script>

    <!-- application -->
    <script src="/js/router.js"></script>
    <script src="/js/ctrl.js"></script>
    <script src="/js/utils.js"></script>
    <script src="/js/websocket.js"></script>
    <script src="/js/main.js"></script>
  </body>
</html>
